// https://raw.githubusercontent.com/nomadcoders/nuber-client/master/tslint.json

{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "max-classes-per-file": false,
    "jsx-no-lambda": false,
    "no-console": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "src/countries.ts"
    ]
  }
}